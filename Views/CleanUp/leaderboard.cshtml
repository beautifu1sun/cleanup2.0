<style>
    .profile-pic{
            width: 100px; 
            height: 100px;
            object-fit: cover;
            border-radius: 50%;
    }
    div{
        border: 2px solid black; 
    }
</style>
<h1>Top 3 Scores: </h1>
<div>
    @for(int i = 0; i < @ViewBag.scorecount; i++){
        <div>
            <a href="/profile/@ViewBag.score[i].UserId"><img class="profile-pic" src="@ViewBag.score[i].ProfilePic" alt=""></a>
            <h3>@ViewBag.score[i].FirstName @ViewBag.score[i].LastName</h3>
            <h3>@ViewBag.score[i].UserName</h3>
            <h3>Score: @ViewBag.score[i].Score</h3>
        </div>
    }
</div>
<h1>Top 3 Tokens</h1>
<div>
    @for(int j = 0; j < @ViewBag.tokencount; j++){
        <div>
            <a href="/profile/@ViewBag.tokens[j].UserId"><img class="profile-pic" src="@ViewBag.tokens[j].ProfilePic" alt=""></a>
            <h3>@ViewBag.tokens[j].FirstName @ViewBag.tokens[j].LastName</h3>
            <h3>@ViewBag.tokens[j].UserName</h3>
            <h3>Tokens: @ViewBag.tokens[j].Token</h3>
        </div>
    }
</div>